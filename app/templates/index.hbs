{{outlet}}
<div class="container">
  <div class="col-md-4 description">
    <div class="jumbotron">
      <h3>How it Works...</h3>
      <ul>
        <li>Click on the Answer button to view the answers.</li>
        <li>Don't agree the listed answers? click new answer to enter yours.</li>
        <li>Click on add new tab to ask a new question</li>
      </ul>
    </div>
  </div>

  <div class="col-md-8 questions">
      <h2>Questions...</h2>
      <ol>
        {{#each model.questions as |question|}}
        {{question-tile question=question }}
        {{/each}}
      </ol>
      <!-- <ul>
      {{#each model.answer as |answer|}}
        <li>{{answer.content}} - by {{answer.author}} {{answer.rating}}</li>
      {{/each}}
    </ul> -->
  </div>

</div>
<nav class="navbar navbar-default navbar-fixed-bottom">
  <div class="newq">
  {{new-question saveQuestion="saveQuestion"}}
</div>
</nav>
